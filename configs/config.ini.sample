LogLevel   = info

[redis]
Host = 127.0.0.1
Port = 6379

[kafkaProducer]
Host = 127.0.0.1
Port = 9092
RequireACKs = WaitForAll
Partitioner = NewRandomPartitioner
ReturnSuccesses = true

[kafkaConsumer]
Host = 127.0.0.1
Port = 9092

[feed]
